<?xml version="1.0" encoding="iso-8859-1" ?>
<Case>
  <Client>Jack Smith, M.D.</Client>
  <CaseType>Substance Abuse</CaseType>
  <CaseSubject>Acute morphine addiction</CaseSubject>
  <CaseStatus>Ongoing</CaseStatus>
  <CaseOpen>September 12, 2008</CaseOpen>
  <ActivitySet>15 Day Review</ActivitySet>
  <IncludeActivities>All</IncludeActivities>
  <Redact>false</Redact>
  <Activity>
      <ActivityType>Open Case</ActivityType>
      <Field>
        <Type>varchar</Type>
        <Label>Reporter</Label>
        <Value>Ann Jones</Value>
      </Field>
      <Field>
        <Type>date</Type>
        <Label>Due Date</Label>
        <Value>September 12, 2008</Value>
      </Field>
      <Field>
        <Type>datetime</Type>
        <Label>Activity Date</Label>
        <Value>September 12, 2008 11:00AM</Value>
      </Field>
      <Field>
        <Type>varchar</Type>
        <Label>Subject</Label>
        <Value>Open case during phone call</Value>
      </Field>
      <Field>
        <Type>int unsigned</Type>
        <Label>Duration</Label>
        <Value>15</Value>
      </Field>
  </Activity>
</Case>


  <field>
      <name>location</name>
      <title>Location</title>
      <import>true</import>
      <type>varchar</type>
      <length>255</length>
      <comment>Location of the activity (optional, open text).</comment>
      <add>1.1</add>
  </field>

  <field>
      <name>phone_id</name>
      <type>int unsigned</type>
      <comment>Phone ID of the number called (optional - used if an existing phone number is selected).</comment>
      <add>2.0</add>
  </field>
  <foreignKey>
       <name>phone_id</name>
       <table>civicrm_phone</table>
       <key>id</key>
       <add>2.0</add>
       <onDelete>SET NULL</onDelete>
  </foreignKey>

  <field>
      <name>phone_number</name>
      <type>varchar</type>
      <length>64</length>
      <comment>Phone number in case the number does not exist in the civicrm_phone table.</comment>
      <add>2.0</add>
  </field>

  <field>
      <name>details</name>
      <title>Details</title>
      <import>true</import> 
      <type>text</type>
      <comment>Details about the activity (agenda, notes, etc).</comment>
      <add>1.1</add>
  </field>

  <field>
      <name>status</name>
      <type>enum</type>
      <values>Scheduled, Completed</values>
      <comment>What is the status of this meeting? Completed meeting status results in activity history entry.</comment>
      <add>1.1</add>
      <drop>2.0</drop>                   
  </field>

  <field>
      <name>status_id</name>
      <title>Activity Status Label</title>
      <import>true</import>
      <type>int unsigned</type>
      <comment>ID of the status this activity is currently in. Foreign key to civicrm_option_value.</comment>
      <add>2.0</add>
  </field>
  <field>
      <name>priority_id</name>
      <type>int unsigned</type>
      <comment>ID of the priority given to this activity. Foreign key to civicrm_option_value.</comment>
      <add>2.0</add>
  </field>
  <field>
      <name>parent_id</name>
      <type>int unsigned</type>
      <comment>Parent meeting ID (if this is a follow-up item). This is not currently implemented</comment>
       <add>1.1</add>
  </field>
  <foreignKey>
       <name>parent_id</name>
       <table>civicrm_activity</table>
       <key>id</key>
       <add>1.1</add>
       <onDelete>CASCADE</onDelete>
  </foreignKey>

  <field>
       <name>is_test</name>
       <title>Test</title>
       <type>boolean</type>
       <default>0</default>
       <import>true</import>
       <add>2.0</add>
  </field>                                 
  <field>
       <name>medium</name>
       <title>Activity Medium</title>
       <type>int unsigned</type>
       <default>NULL</default>
       <comment>Activity Medium, Implicit FK to civicrm_option_value where option_group = activity_medium.</comment>
       <add>2.2</add>
  </field>
  <index>
       <name>index_medium</name>
       <fieldName>medium</fieldName>       
       <add>2.2</add>
  </index>
  <field>
       <name>is_auto</name>
       <title>Auto</title>
       <type>boolean</type>
       <default>0</default>
       <add>2.2</add>
  </field>     
  <field> 
       <name>relationship_id</name>
       <type>int unsigned</type> 
       <default>NULL</default>
       <comment>FK to Relationship ID</comment> 
       <add>2.2</add> 
  </field>
  <foreignKey> 
       <name>relationship_id</name> 
       <table>civicrm_relationship</table> 
       <key>id</key> 
       <onDelete>SET NULL</onDelete>
  </foreignKey>               
  <field>
       <name>activity_tag1_id</name>
       <type>int unsigned</type>
       <required>true</required>
       <comment>Foreign key to the referenced item.</comment>
       <add>2.1</add>
       <hrd>1</hrd>
  </field>
  <index>
       <name>index_activity_tag1_id</name>
       <fieldName>activity_tag1_id</fieldName>
       <add>2.1</add>
       <hrd>1</hrd>
  </index> 
  <field>
       <name>activity_tag2_id</name>
       <type>int unsigned</type>
       <required>true</required>
       <comment>Foreign key to the referenced item.</comment>
       <add>2.1</add>
       <hrd>1</hrd>
  </field>
  <index>
       <name>index_activity_tag2_id</name>
       <fieldName>activity_tag2_id</fieldName>
       <add>2.1</add>
       <hrd>1</hrd>
  </index> 
  <field>
       <name>activity_tag3_id</name>
       <type>varchar</type>
       <length>128</length>
       <required>true</required>
       <comment>Foreign key to the referenced item.</comment>
       <add>2.1</add>
       <hrd>1</hrd>
  </field>
  <index>
       <name>index_activity_tag3_id</name>
       <fieldName>activity_tag3_id</fieldName>
       <add>2.1</add>
       <hrd>1</hrd>
  </index>
</Case>
