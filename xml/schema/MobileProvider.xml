<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM</base>
  <name>crm_mobile_provider</name>
  <comment>Mobile Provider catalogue.</comment>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Mobile Provider ID</comment>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>true</autoincrement>
  </primaryKey>
  <field>
       <name>name</name>
       <type>varchar</type>
       <length>64</length>
       <comment>Name of Mobile Provider.</comment>
  </field>
  <field>
       <name>is_reserved</name>
       <type>boolean</type>
       <comment>Is this entry a predefined system option?</comment>
  </field>
  <field>
       <name>is_active</name>
       <type>boolean</type>
       <comment>Is this entry active?</comment>
  </field>
</table>
