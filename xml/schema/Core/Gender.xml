<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Core</base>
  <class>Gender</class>
  <name>civicrm_gender</name>
  <add>1.2</add>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Gender ID</comment>
       <add>1.2</add>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>true</autoincrement>
  </primaryKey>
  <field>
      <name>domain_id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Which Domain owns this gender.</comment>
       <add>1.2</add>
  </field>
  <foreignKey>
       <name>domain_id</name>
       <table>civicrm_domain</table>
       <key>id</key>
       <add>1.2</add>
  </foreignKey>
  <field>
       <name>name</name>
       <title>Gender</title>
       <type>varchar</type>
       <length>64</length>
       <import>true</import>
       <headerPattern>/^(gender|sex)/i</headerPattern>
       <dataPattern>/^(f|m|(fe)?male)\.?$/i</dataPattern>
       <comment>Gender Name.</comment>
       <add>1.2</add>
  </field>
  <field>
      <name>weight</name>
      <type>int</type>
      <required>true</required>
      <default>1</default>
      <comment>Controls Gender order in the select box.</comment>
       <add>1.2</add>
  </field>
  <field>
       <name>is_active</name>
       <type>boolean</type>
       <comment>Is this property active?</comment>
       <add>1.2</add>
  </field>
  <index>
    <name>UI_name_domain_id</name>
    <fieldName>name</fieldName>
    <fieldName>domain_id</fieldName>
    <unique>true</unique>
   <add>1.2</add>
  </index>
</table>
