<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Core</base>
  <class>IndividualSuffix</class>
  <name>civicrm_individual_suffix</name>
  <add>1.1</add>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Individual Suffix ID</comment>
       <add>1.1</add>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>true</autoincrement>
  </primaryKey>
  <field>
      <name>domain_id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Which Domain owns this individual suffix.</comment>
       <add>1.1</add>
  </field>
  <foreignKey>
       <name>domain_id</name>
       <table>civicrm_domain</table>
       <key>id</key>
       <add>1.1</add>
  </foreignKey>
  <field>
       <name>name</name>
       <title>Individual Suffix</title>
       <type>varchar</type>
       <length>64</length>
       <import>true</import>
       <comment>Individual Suffix Name.</comment>
       <add>1.1</add>
  </field>
  <field>
      <name>weight</name>
      <type>int</type>
      <required>true</required>
      <default>1</default>
      <comment>Controls Individual Suffix order in the select box.</comment>
       <add>1.1</add>
  </field>
  <field>
       <name>is_active</name>
       <type>boolean</type>
       <comment>Is this property active?</comment>
       <add>1.1</add>
  </field>
  <index>
    <name>UI_name_domain_id</name>
    <fieldName>name</fieldName>
    <fieldName>domain_id</fieldName>
    <unique>true</unique>
   <add>1.1</add>
  </index>
</table>
