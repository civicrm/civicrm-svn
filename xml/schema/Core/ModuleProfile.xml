<?xml version="1.0" encoding="iso-8859-1" ?>
<table>
  <base>CRM/Core</base>
  <class>ModuleProfile</class>
  <name>civicrm_module_profile</name>
  <comment>This join table links modules (and sometimes module entities) to profiles (uf_group records). .</comment>
  <add>1.3</add>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Unique ID</comment>
       <add>1.3</add>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>true</autoincrement>
  </primaryKey>
  <field> 
      <name>domain_id</name> 
      <type>int unsigned</type> 
      <required>true</required> 
      <comment>Which Domain owns this module profile entry.</comment> 
       <add>1.3</add> 
  </field> 
  <foreignKey> 
       <name>domain_id</name> 
       <table>civicrm_domain</table> 
       <key>id</key> 
       <add>1.3</add> 
  </foreignKey>
  <field>
      <name>module</name>
      <type>varchar</type>
      <length>255</length>
      <comment>Module Name.</comment>
      <add>1.3</add>
  </field>
  <field> 
       <name>entity_table</name> 
       <type>varchar</type> 
       <length>64</length> 
       <comment>physical tablename for entity being extended by this data, e.g. civicrm_contact</comment> 
       <add>1.3</add> 
  </field> 
  <field> 
      <name>entity_id</name> 
      <type>int unsigned</type> 
      <required>true</required> 
      <comment>FK to record in the entity table specified by entity_table column.</comment> 
      <add>1.3</add> 
  </field>
  <index> 
       <name>index_entity</name> 
       <fieldName>entity_table</fieldName> 
       <fieldName>entity_id</fieldName> 
       <add>1.3</add> 
  </index>
  <field> 
      <name>uf_group_id</name> 
      <type>int unsigned</type> 
      <required>true</required> 
      <comment>Which form does this field belong to.</comment> 
      <add>1.3</add> 
  </field> 
  <foreignKey> 
       <name>uf_group_id</name> 
       <table>civicrm_uf_group</table> 
       <key>id</key> 
       <add>1.3</add> 
  </foreignKey> 
  <field> 
      <name>weight</name> 
      <type>int</type> 
      <required>true</required> 
      <default>1</default> 
      <comment>each internal or external module uses this to order multiple profiles associated with an entity_id</comment> 
      <add>1.3</add> 
  </field> 
</table>
