<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Core</base>
  <class>County</class>
  <name>civicrm_county</name>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
       <comment>County ID</comment>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>false</autoincrement>
  </primaryKey>
  <field>
       <name>name</name>
       <title>Country</title>
       <type>varchar</type>
       <length>64</length>
       <comment>Name of County</comment>
  </field>
  <field>
       <name>abbreviation</name>
       <title>Country Abbreviation</title>
       <type>varchar</type>
       <length>4</length>
       <comment>2-4 Character Abbreviation of County</comment>
  </field>
  <field>
       <name>state_province_id</name>
       <type>int unsigned</type>
       <required>true</required>
       <comment>ID of State / Province that County belongs</comment>
  </field>
  <foreignKey>
       <name>state_province_id</name>
       <table>civicrm_state_province</table>
       <key>id</key>
  </foreignKey>
  <index>
    <name>UI_name_state_id</name>
    <fieldName>name</fieldName>
    <fieldName>state_province_id</fieldName>
    <unique>true</unique>
  </index>
</table>
