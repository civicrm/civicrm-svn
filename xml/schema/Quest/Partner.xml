<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Quest</base>
  <class>Partner</class>
  <name>quest_partner</name>
  <comment>Quest Partner Colleges and Foundations.</comment>
  <add>1.5</add>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Unique Partner ID</comment>
      <add>1.5</add>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>true</autoincrement>
  </primaryKey>
  <field>
       <name>name</name>
       <type>varchar</type>
       <required>true</required>
       <length>64</length>
       <comment>Partner display name.</comment>
       <add>1.5</add>
  </field>
  <field>
       <name>partner_type</name>
       <type>enum</type>
       <values>College, Scholarship</values>
       <required>true</required>
       <comment>Type of partnership (Colleges and Scholarships are listed separately in Forwarding section).</comment>
      <add>1.5</add>
  </field>
  <field>
       <name>url_learn</name>
       <type>varchar</type>
       <length>255</length>
       <comment>URL for learn more links.</comment>
       <add>1.5</add>
  </field>
  <field>
      <name>weight</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Controls display sort order.</comment>
       <add>1.5</add>
  </field>
  <field>
       <name>is_supplement</name>
       <type>boolean</type>
       <default>0</default>
       <comment>Is there an application supplement section for this partner.</comment>
       <add>1.5</add>
  </field>
  <field>
       <name>is_active</name>
       <type>boolean</type>
       <default>1</default>
       <comment>Is this partner active?</comment>
       <add>1.5</add>
  </field>
</table>
