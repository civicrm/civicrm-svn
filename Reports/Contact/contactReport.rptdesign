            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT distinct(contact.id),contact.display_name,
addr.street_name,addr.street_number,
addr.supplemental_address_1,addr.city,addr.state_province_id,
addr.country_id,addr.postal_code,addr.geo_code_1,
addr.geo_code_2,country.name as countryName,
state.name as stateName,e.email,loc.id
FROM civicrm_contact as contact
LEFT JOIN (civicrm_location as loc,
		   civicrm_address as addr,
		   civicrm_state_province as state,
		   civicrm_country as country,
		   civicrm_email as e,
		   civicrm_group_contact as grpContact,
		   civicrm_group as grp )
ON (contact.id=loc.entity_id
	AND loc.id=addr.location_id
	AND addr.state_province_id=state.id
	AND addr.country_id=country.id
	AND loc.id=e.location_id 
	AND e.is_primary=1
	AND contact.id=grpContact.contact_id
	AND grpContact.group_id=grp.id)
WHERE contact.contact_type = ? 
AND grp.name = ?</property>
