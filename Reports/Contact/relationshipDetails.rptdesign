            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT relation.id,relation.contact_id_a,relation.contact_id_b,
relation.relationship_type_id,relation.start_date,relation.end_date,
relation.is_active,relation.description,relType.name_a_b,
contact.display_name
FROM civicrm_relationship as relation, 
civicrm_relationship_type as relType, civicrm_contact as contact
