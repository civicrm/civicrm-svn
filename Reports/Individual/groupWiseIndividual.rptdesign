            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT grp.id,grp.name,grpContact.group_id,
grpContact.contact_id,
grpContact.status,contact.display_name,
contact.contact_type,e.email
FROM civicrm_contact as contact
LEFT JOIN (civicrm_group_contact as grpContact, 
civicrm_group as grp, civicrm_location as loc, civicrm_email as e)
ON (contact.id=grpContact.contact_id
AND grpContact.group_id=grp.id
AND loc.entity_id=contact.id
AND e.location_id=loc.id)
WHERE grpContact.status='Added'</property>
