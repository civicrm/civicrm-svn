            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT distinct(org.id),org.contact_id,org.organization_name,contact.id,
addr.street_name,addr.street_number,addr.supplemental_address_1,
addr.city,addr.state_province_id,addr.country_id,addr.postal_code,
addr.geo_code_1,addr.geo_code_2,country.name as countryName,state.name as stateName,
e.email,loc.id
FROM ((((((civicrm_organization as org
LEFT JOIN civicrm_contact as contact ON org.contact_id=contact.id AND contact.contact_type="Organization")
LEFT JOIN civicrm_location as loc ON contact.id=loc.entity_id)
LEFT JOIN civicrm_address as addr ON loc.id=addr.location_id)
LEFT JOIN civicrm_state_province as state ON addr.state_province_id=state.id)
LEFT JOIN civicrm_country as country ON addr.country_id=country.id)
LEFT JOIN civicrm_email as e ON loc.id=e.location_id)</property>
        </oda-data-set>
