                    <property name="nativeName">contribMonth</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT sum(contrib.total_amount) AS ctAmt,
count(contrib.id) AS ctNum,
avg(contrib.total_amount) AS ctAvg,
date_format(contrib.receive_date,'%Y%m%d') as contribDate,
monthname(contrib.receive_date) AS contribMonth
FROM civicrm_contribution AS contrib,
civicrm_contact AS contact
WHERE contrib.contact_id = contact.id 
AND date_format(contrib.receive_date,'%Y') = ?
GROUP BY contribMonth
ORDER BY month(contrib.receive_date)</property>
        </oda-data-set>
