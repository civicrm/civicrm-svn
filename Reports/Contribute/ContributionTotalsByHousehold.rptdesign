            <property name="queryText">SELECT distinct(contrib.id), r.id as rid,contact_b.display_name,
contrib.total_amount AS contrib_total,
sum(contrib.total_amount) AS ctAmt,
count(contrib.id) AS ctNum,
avg(contrib.total_amount) AS ctAvg
FROM civicrm_contribution AS contrib,
civicrm_contact AS contact_a, civicrm_relationship r,
civicrm_contact AS contact_b
WHERE contrib.contact_id = contact_a.id AND
contrib.is_test = 0 
AND (contact_a.id = r.contact_id_a 
OR contact_a.id = r.contact_id_b)
AND r.relationship_type_id IN (6, 7) 
AND contact_b.id = r.contact_id_b
AND (contrib.receive_date >= ? AND contrib.receive_date &lt;= ?)
GROUP BY contact_b.id</property>
