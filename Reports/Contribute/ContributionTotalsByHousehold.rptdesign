                    <property name="nativeDataType">12</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT contact.id as cid,contact.display_name,contrib.id AS contriID, 
contrib.total_amount AS contrib_total,
sum(contrib.total_amount) AS ctAmt,
count(contrib.id) AS ctNum,contact.contact_type,
avg(contrib.total_amount) AS ctAvg,
date_format(contrib.receive_date,'%d/%m/%y') AS rDate
FROM civicrm_contribution AS contrib,
civicrm_contact AS contact
WHERE contrib.contact_id = contact.id AND
contrib.is_test = 0 AND 
contact.contact_type="Household"
GROUP BY contact.id</property>
