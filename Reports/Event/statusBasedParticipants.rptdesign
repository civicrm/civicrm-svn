            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT p.contact_id,p.event_id,
p.status_id,p.role_id,p.source,p.event_level,
op.name as status,c.display_name,c.id,p.register_date,e.title,
op.id,op.value
FROM civicrm_participant as p
LEFT JOIN (civicrm_contact as c,
civicrm_option_group as grp,
civicrm_option_value as op, 
civicrm_event as e)
ON (
op.option_group_id=grp.id
AND op.value=p.status_id
AND p.contact_id=c.id
AND p.event_id=e.id)
WHERE grp.name="participant_status"
AND op.name = ?</property>
        </oda-data-set>
