            <method name="beforeOpen"><![CDATA[this.queryText = this.queryText + " where date_format(p.register_date,'%c%d%Y') >= " + params["fromDate"] + " and date_format(p.register_date,'%c%d%Y') <= " + params["toDate"] + " and op1.name = '" + params["status"] +"' and op2.name = '" + params["eventType"] +"' and e.title= '" + params["eventName"] +"' and p.is_test = " + params["isTest"] +"";]]></method>
            <property name="queryText">SELECT p.contact_id, p.event_id, p.status_id, 
p.role_id, p.source, p.event_level, op1.name AS statusName,
op2.name AS typeName , c.display_name, c.id, 
date_format(p.register_date,'%c/%d/%Y') as register_date, e.title, op1.id, op1.value, 
date_format(e.start_date,'%c/%d/%Y') as event_start_date, date_format(e.end_date, '%c/%d/%Y') as event_end_date,
op2.id, op2.value, p.is_test
FROM civicrm_participant AS p
LEFT JOIN (
civicrm_contact AS c, 
civicrm_option_group AS grp1, 
civicrm_option_group AS grp2, 
civicrm_option_value AS op1, 
civicrm_option_value AS op2, 
civicrm_event AS e) 
ON ( p.contact_id = c.id
AND p.event_id = e.id 
AND grp1.name = "participant_status"
AND op1.option_group_id = grp1.id
AND op1.value = p.status_id
AND grp2.name = "event_type"
AND op2.option_group_id = grp2.id
AND op2.value = p.event_id) </property>
