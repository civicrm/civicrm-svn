            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT p.contact_id, p.event_id, p.status_id, 
p.role_id, p.source, p.event_level, op1.name AS statusName,
op2.name AS typeName , c.display_name, c.id, 
date_format(p.register_date,'%d/%m/%y') as register_date, e.title, op1.id, op1.value, 
date_format(e.start_date,'%dd/%mm/%yyyy') as startDate, date_format(e.end_date, '%dd/%mm/%yyyy') as endDate,
op2.id, op2.value
FROM civicrm_participant AS p
LEFT JOIN (
civicrm_contact AS c, 
civicrm_option_group AS grp1, 
civicrm_option_group AS grp2, 
civicrm_option_value AS op1, 
civicrm_option_value AS op2, 
civicrm_event AS e) 
ON ( op1.option_group_id = grp1.id
AND op2.option_group_id = grp2.id
AND op1.value = p.status_id
AND op2.value = p.event_id
AND p.contact_id = c.id
AND p.event_id = e.id )
WHERE grp1.name = "participant_status"
AND grp2.name = "event_type"
AND op1.name = ?
AND op2.name = ?
</property>
        </oda-data-set>
