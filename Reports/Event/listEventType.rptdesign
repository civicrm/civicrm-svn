            <property name="dataSource">civicrm</property>
            <property name="queryText">SELECT event.title,event.summary, event.description,event.id,
event.event_type_id,date_format(event.start_date,'%d/%m/%y') as start_date,type.name as contriName, 
date_format(event.end_date,'%d/%m/%y') as end_date,event.max_participants,event.contribution_type_id,
optionVal.name as eventType
FROM civicrm_event as event 
LEFT JOIN (civicrm_option_group as grp,
civicrm_option_value as optionVal, 
civicrm_contribution_type as type) 
ON (optionVal.option_group_id=grp.id 
AND type.id=event.contribution_type_id 
AND event.event_type_id=optionVal.value 
AND grp.name="event_type" ) </property>
        </oda-data-set>
